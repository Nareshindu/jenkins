// pipeline {
//     stages {
//         stage('Naresh') {
//             steps {
//                 sh echo 'hello Naresh Naidu'
//             }
//             }
//         }
//     }
// }

pipeline {
    agent any
    stages {
        stage('Naresh') {
            steps {
                 sh echo 'Hello Naresh'
            }
        }
    }
    post {
        always {
            echo 'print this'
        }
        success {
            echo 'above mentioned pipeline is success'
        }
        falure {
            echo 'above mentioned pipeline is failure'
        }
    }
}